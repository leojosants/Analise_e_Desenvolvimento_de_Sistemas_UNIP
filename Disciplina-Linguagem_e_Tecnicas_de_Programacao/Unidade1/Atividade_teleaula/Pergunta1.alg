algoritmo "semnome"
// Função :
// Autor :
// Data : 18/11/2022
// Seção de Declarações 

var
   n1, n2: real
   p1, p2: real
   media: real

inicio
// Seção de Comandos 
   p1 <- 0.4
   p2 <- 0.6
   
   escreva("Digite a nota 1: ")  // 8
   leia(n1)
   
   escreva("Digite a nota 2: ")  // 6
   leia(n2)
   
   media <- (n1 * p1) + (n2 * p2)
   
   escreva("A media eh: ", media)

fimalgoritmo
